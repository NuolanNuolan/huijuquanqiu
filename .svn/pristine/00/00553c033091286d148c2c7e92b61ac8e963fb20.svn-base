#import <Foundation/Foundation.h>

@interface User : NSObject

@property(nonatomic,   copy) NSString *mbID; // --主键，会员编号
@property(nonatomic,   copy) NSString *mbName;// --会员账号
@property(nonatomic,copy    )NSString *imgURL;//头像地址
@property(nonatomic,   copy) NSString *alipayName;//--支付宝账号
@property(nonatomic,   copy) NSString *mbPwd;// --会员密码
@property(nonatomic,   copy) NSString *mob; // --会员手机号
@property(nonatomic,   copy) NSString *mbDetailID; // --会员详细信息ID
@property(nonatomic,   copy) NSString *presLog; // --最近登录时间
@property(nonatomic,   copy) NSString *mbBalance;// --会员余额
@property(nonatomic,   copy) NSString *isCancel;// --是否注销(挂失)
@property(nonatomic,   copy) NSString *isLock; // --是否锁定（封号）
@property(nonatomic,   copy) NSString *email;   //--邮箱


@end
